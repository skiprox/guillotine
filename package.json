{
  "name": "single-interaction-site",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node run-single-interaction.js",
    "build": "npm run css && npm run js",
    "watch": "npm run build && parallelshell \"npm run watch:css\" \"npm run watch:js\"",
    "css": "node-sass src/scss/main.scss public/css/main.css --output-style compressed",
    "watch:css": "onchange 'src/scss/*.scss' -- npm run css",
    "js": "browserify src/js/app.js -o public/js/app.js && browserify src/js/head.js -o public/js/head.js",
    "watch:js": "onchange 'src/js/*.js' -- npm run js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.16.3",
    "gsap": "^2.0.2",
    "no-scroll": "^2.1.1",
    "socket.io": "^2.1.1"
  },
  "devDependencies": {
    "browserify": "^16.2.2",
    "node-sass": "^4.9.3",
    "onchange": "^4.1.0",
    "parallelshell": "^3.0.2"
  },
  "browser": {
    "tween-lite": "./node_modules/gsap/src/uncompressed/TweenLite.js",
    "timeline-lite": "./node_modules/gsap/src/uncompressed/TimelineLite.js",
    "ease-pack": "./node_modules/gsap/src/uncompressed/easing/EasePack.js",
    "css-plugin": "./node_modules/gsap/src/uncompressed/plugins/CssPlugin.js",
    "scroll-to-plugin": "./node_modules/gsap/src/uncompressed/plugins/ScrollToPlugin.js"
  }
}
